{
  "name": "nr-labs-teams-sync-action",
  "version": "0.1.0",
  "description": "GitHub action to synchronize New Relic teams from config files",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "bundle": "npm run package",
    "ci-test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint . && npx markdownlint '**/*.md'",
    "local-action": "npx @github/local-action . src/main.ts .env",
    "package": "npx rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "all": "npm run format:check &&npm run lint && npm run test && npm run coverage && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newrelic-experimental/nr-labs-teams-sync-action.git"
  },
  "author": "sdewitt@newrelic.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/newrelic-experimental/nr-labs-teams-sync-action/issues"
  },
  "homepage": "https://github.com/newrelic-experimental/nr-labs-teams-sync-action#readme",
  "exports": {
    ".": "./dist/index.js"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@github/local-action": "^6.0.2",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "make-coverage-badge": "^1.2.0",
    "markdownlint": "^0.40.0",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "rollup": "^4.53.2",
    "ts-jest": "^29.4.5",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/http-client": "^3.0.0"
  }
}
